<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper>
    <select id="findCommodityBySku" resultMap="com.test.entity.Commodity">
        select * from commodity where sku=#{sku}
    </select>
</mapper>